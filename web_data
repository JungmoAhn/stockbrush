<script type="text/javascript">
function mouseOver(obj) {
  obj.style.backgroundColor="#f6f4e5";
}

function mouseOut(obj) {
  obj.style.backgroundColor="#ffffff";
}

function select_type(type) {
	document.getElementById(type).style.display = '';
	document.getElementById(type+'_title').className = 'p11 red03 u';
	
	if(type == 'frgn_chart') {
		document.getElementById('organ_chart').className = '';
		document.getElementById('organ_chart_title').className = 'p11';
		document.getElementById('organ_chart').style.display = 'none';
	} else {
		document.getElementById('frgn_chart').className = '';
		document.getElementById('frgn_chart_title').className = 'p11';
		document.getElementById('frgn_chart').style.display = 'none';
	}
}
		
function select_gigan(type, pst) {
	for(var i=0 ; i<4 ; i++) {
		var img_pst = i+1;
				
		if (i == pst) {
			document.getElementById(type+'_'+pst).className = 'on';
			document.getElementById(type+'_img_'+pst).src = 'http://imgstock.naver.net/images5/stab01_0'+img_pst+'_on.gif';
		} else {
			document.getElementById(type+'_'+i).className = "";
			document.getElementById(type+'_img_'+i).src = 'http://imgstock.naver.net/images5/stab01_0'+img_pst+'_off.gif';
		}
	}
}
</script>

		
